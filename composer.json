{
    "name": "lepo/core",
    "description": "PHP dependency injection and hosting",
    "type": "library",
    "license": "MIT",
    "version": "0.1.0",
    "keywords": [
        "di",
        "asp.net",
        "dependency",
        "injection",
        "hosting",
        "routing",
        "net",
        "service",
        "list",
        "monitoring",
        "monitorowanie",
        "packages",
        "package"
    ],
    "autoload": {
        "psr-4": {
            "Lepo\\Core\\": "src/"
        },
        "classmap": [
            "src/"
        ]
    },
    "authors": [
        {
            "name": "Leszek Pomianowski"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.1 || ^8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
